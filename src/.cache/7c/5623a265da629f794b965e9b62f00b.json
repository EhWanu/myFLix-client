tructor() {\r\n    super();\r\n// Initial state is set to null\r\n    this.state = {\r\n      movies: null,\r\n      selectedMovie: null,\r\n      user: null\r\n    };\r\n  }\r\n  componentDidMount(){\r\n    axios.get('https://camsmyflic.herokuapp.com/movies')\r\n    .then(response => {\r\n      this.setState({\r\n        movies: response.data\r\n      });\r\n    })\r\n    .catch(error => {\r\n      console.log(error);\r\n    });\r\n  }\r\n\r\n  onMovieClick(movie) {\r\n    this.setState({\r\n      selectedMovie: movie\r\n    });\r\n  }\r\n\r\n  onLoggedIn(user) {\r\n    this.setState({\r\n      user\r\n    });\r\n  }\r\n\r\n\r\n  setSelectedMovie(newSelectedMovie) {\r\n    this.setState({\r\n      selectedMovie: newSelectedMovie\r\n    });\r\n  }\r\n\r\n  render() {\r\n    const { movies, selectedMovie, user } = this.state;\r\n\r\n    if (!user) return <LoginView onLoggedIn={user => this.onLoggedIn(user)} />;\r\n\r\n    // Before the movies have been loaded\r\n    if (!movies) return <div className=\"main-view\"/>;\r\n    \r\n    if (selectedMovie) return <MovieView movie={selectedMovie} onBackClick={newSelectedMovie => { this.setSelectedMovie(newSelectedMovie); }} />;\r\n\r\n    if (movies.length === 0) return <div className=\"main-view\"/>\r\n\r\n    return (\r\n      <div className=\"main-view\">\r\n        {movies.map(movie => <MovieCard key={movie._id} movie={movie} onMovieClick={newSelectedMovie => { this.setState({ selectedMovie: newSelectedMovie }); }} />)}\r\n      </div>\r\n    );\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"5814a0d19b84840e66db1e56062f4970","cacheData":{"env":{}}}